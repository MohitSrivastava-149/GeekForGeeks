class Solution {
    static int nthFibonacci(int n){
        int m = 1000000007;
        int num1 = 0;
        int num2 = 1;
        int res = num1+num2;
        
        for(int i=2; i<=n; i++){
            res = (num1+num2)%m;
            num1 = num2;
            num2 = res;
        }
        return res;
    }
}
